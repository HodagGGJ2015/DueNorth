{
  "global": {
    "init": [{
      "global": {
        "$set": {
          "name": "Welcome to Wisconsin",
          "description": "${global.name}",
          "response": "What would you like to do?",
          "image": ""
        }
      }
    }],
    "invalid": [{
      "global": {
        "$merge": {
          "response": "Cannot do that."
        }
      }
    }],
    "help": [{
      "global": {
        "$merge": {
          "response": "You asked for something?"
        }
      }
    }]
  },
  "player": {
    "init": [{
      "player": {
        "$set": {
          "name": "Jim",
          "location": "motorhome"
        }
      }
    }]
  },
  "inventory": {
    "init": [{
      "inventory": {
        "$set": {}
      }
    }]
  },
  "skunk": {
    "init": [{
      "skunk": {
        "$set": {
          "location": "motorhome"
        }
      }
    }],
    "take": [{
      "inventory": {
        "$eq": {
          "skunk": false
        }
      }
    }, {
      "skunk": {
        "$merge": {
          "mood": "angry",
          "location": "inventory"
        }
      }
    }, {
      "inventory": {
        "$merge": {
          "skunk": true
        }
      }
    }, {
      "player": {
        "$increment": {
          "health": 10
        }
      }
    }, {
      "global": {
        "$merge": {
          "response": "The skunk is in ${skunk.location} and is ${skunk.mood} because of ${player.name} whose health is ${player.health}."
        }
      }
    }]
  },
  "motorhome": {
    "init": [
      "$set": {
        "visited": false,
        "fullDescription": "A 1970 Winnebago Chieftain, the pride of your family name. Beige body with a large brown W on the side that your dad keep as polished as the day he first bought it. Sometimes you think he loves the Winnebago more than you.",
        "shortDescription": "Your family's motorhome.",
        "directions": {
          "north": "motel",
          "south": "woods"
        }
      }
    ]
  }
}
