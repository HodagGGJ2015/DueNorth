tbody {
  background-color #ccc
  margin 10px
}

.debug {
  font-family "Courier"
  font-size 12px
  color #33D
}

#content {
  position relative
  top 5%
  left 5%
  width 90%
  height 400px
  border 2px solid black
  // background-color #eee
  z-index 1
  
  img {
  }

  .title {
    position absolute
    top 15px
    left 15px
    padding 5px
    border 2px solid black
    background-color #f1f7f0
    color black
    font-family "Oswald"
    font-size 16px
    z-index 20
  }

  #alert {
    position absolute
    top 50px
    left 15px
    border 4px solid black
    background-color white
    color black
    border-radius 10px
    padding 10px
    z-index 100
    // display none
  }

  #iframeX {
    position absolute
    top 0
    left 0
    margin 2%
    width 96%
    height 96%
    display none
    z-index 20
  }

}

#sceneInfo {
  position relative
  border 1px solid red
  margin 5%

  input {
    font-size 30px
    height 30px
    z-index 100
  }
  
}

